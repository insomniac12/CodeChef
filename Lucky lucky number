#include <stdio.h>
int main(void)
{
long int t,r,c,q;
long int n;
scanf("%ld",&t);
while(t--)
{
 
scanf("%ld",&n);
r=n%7;
q=n/7;
 if(r==4)
c=7*q;
else if(r==0 || r==1 || r==2 || r==3 )
c=7*(q-r);
else if(r==5)
c=7*(q-1);
else if(r==6)
c=7*(q-2);
if(c<0)
printf("-1\n");
else
printf("%ld\n",c);
}
	return 0;
}
 
