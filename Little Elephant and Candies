#include <stdio.h>
 
int main(void) {
	int t,n,c,i;
	scanf("%d",&t);
    while(t--)
    {
    	scanf("%d\t%ld",&n,&c);
    	long a[n],s=0;
    	for(i=0;i<n;i++)
    	scanf("%d",&a[i]);
    	for(i=0;i<n;i++)
    	s+=a[i];
    	if(s<=c)
    	printf("Yes\n");
    	else
    	printf("No\n");
    }
	return 0;
}
