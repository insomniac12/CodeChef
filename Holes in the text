#include<stdio.h>
#include<string.h>
int main()
{
char str[100];
int t,i,j,c;
scanf("%d",&t);
for(i=0;i<t;i++)
{
c=0;
scanf("%s",str);
for(j=0;str[j]!='\0';j++)
{
if(str[j]=='A'||str[j]=='D'||str[j]=='O'||str[j]=='P'||str[j]=='R'||str[j]=='Q')
c+=1;
if(str[j]=='B')
c+=2;
}
printf("%d\n",c);
}
return 0;
} 
