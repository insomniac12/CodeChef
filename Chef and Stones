#include <iostream>
using namespace std;
#include<bits/stdc++.h>
int main() 
{
int t;
cin>>t;
long long int n,k;
while(t--)
{
cin>>n>>k;
long long int a[n],b[n],i;
for(i=0;i<n;i++)
cin>>a[i];
for(i=0;i<n;i++)
cin>>b[i];
long long int m=0;
for(i=0;i<n;i++)
m=max(m,b[i]*(k/a[i]));
cout<<m<<endl;
}
	return 0;
} 
